<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ManageWishlist</servlet-name>
        <servlet-class>controller.ManageWishlist</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteWl</servlet-name>
        <servlet-class>controller.DeleteWl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeShopController</servlet-name>
        <servlet-class>controller.HomeShopController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListSpByShop</servlet-name>
        <servlet-class>controller.ListSpByShop</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EventController</servlet-name>
        <servlet-class>controller.EventController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ManageWishlist</servlet-name>
        <url-pattern>/ManageWishlist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteWl</servlet-name>
        <url-pattern>/deletewl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeShopController</servlet-name>
        <url-pattern>/homeshop</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListSpByShop</servlet-name>
        <url-pattern>/shopListSp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EventController</servlet-name>
        <url-pattern>/event</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
